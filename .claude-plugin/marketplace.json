{
    "name": "fractary-claude-plugins",
    "owner": {
      "name": "The Fractary",
      "email": "admin@fractary.com",
      "url": "https://github.com/fractary"
    },
    "metadata": {
      "description": "Production-ready workflow orchestration with claude code agents, commands, skills and hooks.",
      "version": "1.0.0", 
      "pluginRoot": "./plugins"
    },
    "plugins": [
      {
        "name": "fractary-codex",
        "source": "./plugins/codex",
        "description": "Project documentation and org wide syncronization to and from organization central codex",
        "version": "1.0.0",
        "author": {
          "name": "The Fractary",
          "url": "https://github.com/fractary"
        },
        "homepage": "https://github.com/fractary/claude-agents/plugins/codex",
        "repository": "https://github.com/fractary/claude-agents/plugins/codex",
        "license": "MIT",
        "keywords": [
          "documentation"
        ],
        "category": "documentation",
        "strict": false,
        "commands": [
          "./commands/doc-generate.md",
          "./commands/code-explain.md"
        ],
        "agents": [
          "./agents/docs-architect.md",
          "./agents/tutorial-engineer.md",
          "./agents/code-reviewer.md"
        ]
      },
      {
        "name": "fractary-repo",
        "source": "./plugins/repo",
        "description": "Universal source control operations across GitHub, GitLab, and Bitbucket with modular handler architecture",
        "version": "2.0.0",
        "author": {
          "name": "The Fractary",
          "url": "https://github.com/fractary"
        },
        "homepage": "https://github.com/fractary/claude-plugins/tree/main/plugins/repo",
        "repository": "https://github.com/fractary/claude-plugins/tree/main/plugins/repo",
        "license": "MIT",
        "keywords": [
          "git",
          "github",
          "gitlab",
          "bitbucket",
          "source-control",
          "repository",
          "version-control",
          "branches",
          "commits",
          "pull-requests",
          "tags"
        ],
        "category": "development",
        "strict": false,
        "commands": [
          "./commands/branch.md",
          "./commands/commit.md",
          "./commands/push.md",
          "./commands/pr.md",
          "./commands/tag.md",
          "./commands/cleanup.md"
        ],
        "agents": [
          "./agents/repo-manager.md"
        ]
      }
    ]
  }