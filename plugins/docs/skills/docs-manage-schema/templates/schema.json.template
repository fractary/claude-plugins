{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "$id": "{{schema_id}}",
  "title": "{{title}}",
  "description": "{{description}}",
  "type": "object",
  "version": "{{version}}",
  "properties": {
    {{#fields}}
    "{{name}}": {
      "type": "{{type}}",
      "description": "{{description}}"{{#format}},
      "format": "{{format}}"{{/format}}{{#pattern}},
      "pattern": "{{pattern}}"{{/pattern}}{{#minimum}},
      "minimum": {{minimum}}{{/minimum}}{{#maximum}},
      "maximum": {{maximum}}{{/maximum}}{{#minLength}},
      "minLength": {{minLength}}{{/minLength}}{{#maxLength}},
      "maxLength": {{maxLength}}{{/maxLength}}{{#enum}},
      "enum": [{{#enum}}"{{.}}"{{^last}}, {{/last}}{{/enum}}]{{/enum}}
    }{{^last}},{{/last}}
    {{/fields}}
  },
  "required": [
    {{#fields}}
    {{#required}}"{{name}}"{{^last}},{{/last}}{{/required}}
    {{/fields}}
  ],
  "additionalProperties": false,
  "examples": [
    {{#examples}}
    {{examples}}{{^last}},{{/last}}
    {{/examples}}
  ]
}
